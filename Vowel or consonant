#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main() {
    char ch;
    scanf("%c",&ch);
        ch = toupper(ch);
        if (ch== 'A' || ch== 'E' || ch== 'I' || ch== 'O' || ch== 'U' )
            printf("Vowel");
        else if (!isalpha(ch))
            printf("Invalid Input");
        else
            printf("Consonant");
        
     return 0;
}
