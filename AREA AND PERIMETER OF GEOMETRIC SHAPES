#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float r,a,l,w,area,perimeter;
    scanf("%f", &r);
    area=3.1416*r*r;
    perimeter=2*3.1416*r;
    printf("Circle: %.2f %.2f\n", area,perimeter);
    scanf("%f", &a);
    area=a*a;
    perimeter=4*a;
    printf("Square: %.2f %.2f\n", area,perimeter);
    scanf("%f%f", &l,&w);
    area=l*w;
    perimeter=2*(l+w);
    printf("Rectangle: %.2f %.2f\n", area,perimeter);
    return 0;
}
