#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int physics,chemistry,bio,math,computer;
    scanf("%d %d %d %d %d", &physics,&chemistry,&bio,&math,&computer);
    int total = physics + chemistry + bio + math + computer;
    float Percentage = (total / 500.0 * 100);
    printf("Percentage = %.2f\n", Percentage);
    if(Percentage>= 90)
        printf("Grade = A\n");
    else if (Percentage >= 80 && Percentage < 90)
        printf("Grade = B\n"); 
    else if (Percentage >= 70 && Percentage < 80)
        printf("Grade = C\n");
    else if (Percentage >= 60 && Percentage < 70)
        printf("Grade = D\n");
    else if (Percentage >= 40 && Percentage < 60)
        printf("Grade = E\n");
    else
        printf("Grade = F\n");
    
    
    
    
    return 0;
}

